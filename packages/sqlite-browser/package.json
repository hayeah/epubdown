{
  "name": "@hayeah/sqlite-browser",
  "version": "0.1.0",
  "type": "module",
  "description": "SQLite for browser with IndexedDB persistence",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./dist/index.js"
    },
    "./dist/index.js": "./dist/index.js",
    "./test": {
      "types": "./test/index.ts",
      "import": "./test/index.ts"
    },
    "./test/nukeIndexedDBDatabases": {
      "types": "./test/nukeIndexedDBDatabases.ts",
      "import": "./test/nukeIndexedDBDatabases.ts"
    }
  },
  "files": ["dist", "src", "test"],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "test:browser": "vitest run -c vitest.config.browser.ts",
    "typecheck": "tsc --noEmit",
    "check": "biome check --write"
  },
  "dependencies": {
    "wa-sqlite": "https://github.com/rhashimoto/wa-sqlite#a883e224392667d45d2a0c42e2c0dcc1f6abcf29"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-node-polyfills": "catalog:",
    "vitest": "catalog:"
  }
}
